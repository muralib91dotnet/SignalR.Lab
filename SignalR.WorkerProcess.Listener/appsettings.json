{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SignalRHubUrl": "http://localhost:5000/valuesHub",
  "NServiceBusSettings": {
    //Target Queue Name(can be RabbitMq/Azure/MSMQ/Any type of Queue, etc)
