    "ToEndpointQueue": "example.server",
    "ServiceBusTransportString": "host=localhost",
    "ImmediateNumberOfRetries": 0,
    "DelayedNumberOfRetries": 3,
    "DelayTimeSpanInSeconds": 10
  }
}
